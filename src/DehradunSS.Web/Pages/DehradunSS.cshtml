                        res.text().then(function (data) {
                            var data = JSON.parse(data);
                            ATMCount = data.features.length;
                            console.log(ATMCount);
                            FetchPostOff();
                        })
                    });
            }

            function FetchPostOff() {
                fetch('http://localhost:8080/geoserver/Dehradun28/ows?service=WFS&version=1.0.0&request=GetFeature&typeName=Dehradun28%3Apostoffice&maxFeatures=50&outputFormat=application%2Fjson')
